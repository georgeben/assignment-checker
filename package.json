{
  "name": "assignment-checker",
  "version": "1.0.0",
  "description": "Compare assignments/essays submitted by students for similarity",
  "main": "index.js",
  "scripts": {
    "build": "babel src --delete-dir-on-start --copy-files  -d dist",
    "prestart": "npm run build",
    "start:dev": "nodemon \"src/index.js\"",
    "start": "cross-env NODE_ENV=production pm2-runtime start ecosystem.config.js --env production",
    "test": "NODE_ENV=test nyc --reporter=text --reporter=html --reporter=lcov mocha --require @babel/register \"src/**/**/*.spec.js\" --exit --timeout 20000 --recursive",
    "lint": "node_modules/.bin/eslint \"./src/**/*.js\""
  },
  "author": "George Benjamin",
  "license": "ISC",
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "awilix": "^4.3.3",
    "awilix-express": "^4.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "convict": "^6.0.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv-extended": "^2.9.0",
    "express": "^4.17.1",
    "fs-extra": "^10.0.0",
    "helmet": "^4.5.0",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mocha": "^7.0.0",
    "moment": "^2.29.1",
    "mongoose": "^5.12.7",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "natural": "^5.0.3",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0",
    "pm2": "^5.1.0",
    "stopword": "^1.0.7",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.13.16",
    "@babel/node": "^7.13.13",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/register": "^7.13.16",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.25.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-import-resolver-babel-module": "^5.3.0",
    "eslint-plugin-import": "^2.22.1"
  }
}
